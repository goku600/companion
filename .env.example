# -------------------------------------------------------
# Copy this file to .env and fill in your real values.
# Never commit .env to version control!
# -------------------------------------------------------

# Telegram Bot Token (from @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN

# Atlassian credentials
ATLASSIAN_EMAIL=your-email@example.com
ATLASSIAN_API_KEY=YOUR_ATLASSIAN_API_KEY

# Your Atlassian site URL, e.g. https://yourcompany.atlassian.net
ATLASSIAN_SITE_URL=https://yourcompany.atlassian.net/rovodev

# Optional: Restrict bot access to specific Telegram user IDs
# Comma-separated list of numeric IDs. Leave blank to allow everyone.
ALLOWED_TELEGRAM_USER_IDS=
