# -------------------------------------------------------
# Copy this file to .env and fill in your real values.
# Never commit .env to version control!
# -------------------------------------------------------

# Telegram Bot Token (from @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN

# OpenRouter API Key (from https://openrouter.ai/keys)
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY

# OpenRouter model to use (set to auto for best free model)
# Browse free models at: https://openrouter.ai/models?q=free
# Example: meta-llama/llama-3.3-70b-instruct:free
OPENROUTER_MODEL=auto

# Optional: Restrict bot access to specific Telegram user IDs
# Comma-separated list of numeric IDs. Leave blank to allow everyone.
ALLOWED_TELEGRAM_USER_IDS=
